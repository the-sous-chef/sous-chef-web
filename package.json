    },
    "sideEffects": false,
    "scripts": {
        "build": "npm run build:tailwind && npm run build:remix",
        "build:remix": "remix build",
        "build:tailwind": "tailwindcss --minify -o app/styles/tailwind.css",
        "cleanup": "rimraf .cache ./build",
        "deploy": "wrangler publish",
        "dev": "cross-env NODE_ENV=development npm-run-all build:remix --parallel \"dev:*\"",
        "dev:tailwind": "tailwindcss -o app/styles/tailwind.css --watch",
        "dev:wrangler": "wrangler pages dev ./public --local --compatibility-date=2023-03-27",
        "prebuild": "npm run cleanup",
        "start": "cross-env NODE_ENV=production npm run wrangler --compatibility-date=2023-03-27",
        "typecheck": "tsc"
    },
    "type": "module",
