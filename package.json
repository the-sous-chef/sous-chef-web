    "scripts": {
        "dev:client": "concurrently \"npm run typecheck\" \"vite dev -c vite.client.config.ts --mode development\"",
        "dev:server": "vite build -c vite.server.config.ts --mode development --watch",
        "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
        "build:client": "vite build -c vite.client.config.ts",
        "build:client:dev": "vite build -c vite.client.config.ts --mode development",
        "build:server": "vite build -c vite.server.config.ts",
        "build:server:dev": "vite build -c vite.server.config.ts --mode development",
        "format": "npm run lint -- --fix",
        "lint": "eslint .",
        "prepare": "is-ci || sh ./scripts/prepare.sh",
        "preview": "vite preview",
        "test": "vitest",
        "test:coverage": "vitest run --coverage",
        "typecheck": "tsc --noEmit"
    },
    "type": "module",
